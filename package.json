{
  "name": "sanity-gatsby-blog-web",
  "version": "1.0.0",
  "private": true,
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "start": "BUILD_ENV=dev gatsby develop",
    "start:alpaca": "GATSBY_APP_NAME=alpaca yarn start",
    "start:llama": "GATSBY_APP_NAME=llama yarn start",
    "start:sheep": "GATSBY_APP_NAME=sheep yarn start",
    "build": "gatsby build",
    "dev": "yarn clean && yarn start",
    "build:alpaca:release": "GATSBY_APP_NAME=alpaca BUILD_ENV=production gatsby build",
    "build:llama:release": "GATSBY_APP_NAME=llama BUILD_ENV=production gatsby build",
    "build:sheep:release": "GATSBY_APP_NAME=sheep BUILD_ENV=production gatsby build",
    "build:alpaca:dev": "GATSBY_APP_NAME=alpaca BUILD_ENV=dev gatsby build",
    "build:llama:dev": "GATSBY_APP_NAME=llama BUILD_ENV=dev gatsby build",
    "build:sheep:dev": "GATSBY_APP_NAME=sheep BUILD_ENV=dev gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "prepare": "husky install",
    "lint": "eslint --cache --fix --ext .tsx --ext .ts src --max-warnings 0",
    "format": "prettier --write \"**/*.+(json)\""
  },
  "dependencies": {
    "@sanity/block-content-to-react": "^3.0.0",
    "@sanity/image-url": "^1.0.1",
    "date-fns": "1.30.1",
    "gatsby": "^3.13.1",
    "gatsby-plugin-image": "^1.14.0",
    "gatsby-plugin-postcss": "^5.1.0",
    "gatsby-plugin-react-helmet": "^5.2.0",
    "gatsby-plugin-react-svg": "^3.0.1",
    "gatsby-plugin-root-import": "^2.0.7",
    "gatsby-plugin-sass": "^4.14.0",
    "gatsby-plugin-sharp": "^3.14.0",
    "gatsby-source-filesystem": "^3.14.0",
    "gatsby-source-sanity": "^7.1.0",
    "gatsby-transformer-sharp": "^3.14.0",
    "lodash": "^4.17.21",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "5.2.1",
    "react-player": "2.7.0",
    "sass": "^1.42.1",
    "typescript": "^4.4.3"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.177",
    "@types/node": "^16.11.9",
    "@types/react": "^17.0.35",
    "@types/react-dom": "^17.0.11",
    "@types/react-helmet": "^6.1.4",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/eslint-plugin-tslint": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "gatsby-plugin-webpack-bundle-analyser-v2": "^1.1.26",
    "husky": "^7.0.0",
    "lint-staged": "^11.2.3",
    "node-filter-async": "2.0.0",
    "stylelint": "^13.13.1",
    "stylelint-a11y": "^1.2.3",
    "stylelint-config-prettier": "^9.0.2",
    "stylelint-config-sass-guidelines": "^8.0.0",
    "stylelint-declaration-use-variable": "^1.7.3",
    "stylelint-high-performance-animation": "^1.5.2",
    "stylelint-prettier": "^1.2.0"
  }
}
